    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.1",
    "react": "18.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-virtuoso": "^4.3.7"
  },
  "loki": {
    "fetchFailIgnore": "https://mockapi.com/*",
    "diffingEngine": "gm",
    "gm": {
      "tolerance": 0.00024
    },
    "verboseRenderer": true,
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
