{
  "name": "react-production",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:dev:server\" \"npm run start:dev:client\"",
    "start:dev:client": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production apiUrl=https://react-production-json-server.vercel.app/",
    "build:prod:analyze": "webpack --env mode=production analyze=true",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" && npx prettier --check \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix && npx prettier --write \"**/*.{ts,tsx}\"",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test --chromeConcurrency 10",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "concurrently -s first -k \"npm run storybook\" \"npm run test:ui\"",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report:chrome": "npm run test:ui:json && npm run test:ui:html && open -a 'Google Chrome' .loki/report.html",
    "test:ui:report:arc": "npm run test:ui:json && npm run test:ui:html && open -a 'Arc' .loki/report.html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook --no-open",
    "build:storybook": "storybook build -c ./config/storybook",
    "generate:slice": "ts-node ./scripts/createSlice/index.ts",
    "rename:slice": "ts-node ./scripts/renameSlice/index.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.21.5",
    "@loki/create-async-callback": "^0.31.1",
    "@loki/is-loki-running": "^0.31.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-a11y": "^7.0.12",
    "@storybook/addon-actions": "^7.0.12",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-webpack5": "^7.0.12",
    "@storybook/testing-library": "^0.1.0",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-loader": "^9.1.2",
    "babel-plugin-i18next-extract": "^0.9.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.4",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-i18next": "^6.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-netliukh-demian-fsd-plugin": "^0.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
